<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.athi.rock.administrateur.Musique.MusiqueAdminFragment"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/colorBackground">

    <TextView
        android:layout_marginTop="42dp"
        android:id="@+id/supprimerMusique"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Supprimer Musique"
        android:textColor="@color/colorAccent"
        android:textSize="20dp"
        android:textStyle="bold" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="retour Home"
        android:id="@+id/btn_retour_utilisateur"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@id/supprimerMusique"/>
    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_retour_utilisateur"
        android:id="@+id/id_listViewMusique_supprimer"
        android:layout_marginBottom="55dp"
        android:layout_marginTop="5dp">
    </ListView>
</RelativeLayout>
